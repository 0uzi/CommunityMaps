<map proto="1.4.0">
<name>Alpine Mining II</name>
<version>0.4</version>
<objective>Destroy the enemy's 2 monuments!</objective>
<authors>
    <author uuid="fe3608b7-d105-4029-8800-34b3147065b6"/> <!-- rockymine -->
</authors>
<contributors>
    <contributor uuid="430ec559-364a-4363-ac7a-2529050440ac" contribution="XML"/> <!-- mame_BT -->
    <contributor uuid="e2d2c2c6-cea9-4510-9ab2-a091b5605b30" contribution="Quality of life additions to XML (bulk crafting)"/> <!-- Ruediger_LP -->
    <contributor uuid="ced434fd-94ce-4750-8aab-86da9914d3a2" contribution="Building and feedback"/> <!-- _Its__me_ -->
    <contributor uuid="dff980ed-9f69-491e-b79c-30c21350baf2" contribution="Original map"/> <!-- DoomyBoy -->
    <contributor uuid="c2848337-b526-4db0-9130-fabd0a5c7cfb" contribution="Original map"/> <!-- Rasta_Jol -->
</contributors>
<created>2021-04-24</created>
<broadcasts>
    <tip after="30s" every="8m">Mine lapis and smelt ores for experience, you can use it to enchant your tools!</tip>
    <tip after="38s" every="8m">The enchantment room is in the back of your team's side, near the mine entrance!</tip>
</broadcasts>
<teams>
    <team id="green" color="dark green" max="45" max-overfill="50">Green</team>
    <team id="orange" color="gold" max="45" max-overfill="50">Orange</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" material="iron sword"/>
        <item slot="1" material="bow">
            <enchantment>infinity</enchantment>
        </item>
        <item slot="28" material="arrow"/>
        <item slot="2" material="diamond pickaxe">
            <enchantment level="3">unbreaking</enchantment>
        </item>
        <item slot="3" material="iron axe">
            <enchantment>efficiency</enchantment>
            <enchantment level="3">unbreaking</enchantment>
        </item>
        <item slot="30" material="iron spade">
            <enchantment level="2">efficiency</enchantment>
        </item>
        <item slot="4" amount="64" material="wood" damage="1"/>
        <item slot="31" amount="32" material="wood" damage="1"/>
        <item slot="5" amount="64" material="stone" damage="5"/>
        <item slot="32" amount="32" material="stone" damage="5"/>
        <item slot="7" material="golden apple"/>
        <item slot="8" amount="64" material="golden carrot"/>
        <chestplate team-color="true" unbreakable="true" material="leather chestplate"/>
        <leggings team-color="true" unbreakable="true" material="leather leggings"/>
    </kit>
    <kit id="kill-reward">
       <effect duration="4" amplifier="2">speed</effect>
    </kit>
</kits>
<spawns>
    <default yaw="-90">
        <region>
            <cuboid min="-82.5,12.5,-11.5" max="-81.5,12.5,-10.5"/>
        </region>
    </default>
    <spawn team="green" kit="spawn-kit" yaw="0">
        <region>
            <cuboid min="-40.5,29.5,128.5" max="-41.5,29.5,127.5"/>
        </region>
    </spawn>
    <spawn team="orange" kit="spawn-kit" yaw="180">
        <region>
            <cuboid min="41.5,29.5,-164.5" max="42.5,29.5,-163.5"/>
        </region>
    </spawn>
</spawns>
<filters>
    <deny id="deny-players-tnt">
        <any>
            <team>green</team>
            <team>orange</team>
            <cause>explosion</cause>
        </any>
    </deny>
    <deny id="deny-beacon-use">
        <material>beacon</material>
    </deny>
    <deny id="deny-trapdoor-use">
        <material>trap door</material>
    </deny>
    <deny id="no-break">
        <any>
            <material>enchantment table</material>
            <material>skull</material>
        </any>
    </deny>
    <all id="only-mineable">
        <any id="mineable">
            <material>gold block</material>
            <material>gold ore</material>
            <material>iron block</material>
            <material>iron ore</material>
        </any>
    </all>
</filters>
<regions>
    <union id="spawns">
        <cuboid id="green-spawn" min="-46,13,125" max="-31,59,154"/>
        <cuboid id="orange-spawn" min="47,13,-161" max="32,59,-190"/>
    </union>
    <union id="enchantment-huts">
        <rectangle id="green-hut" min="35,212" max="47,221"/>
        <rectangle id="orange-hut" min="-34,-214" max="-46,-223"/>
    </union>
    <rectangle id="obs-spawn" min="-61,2" max="-101,-38"/>
    <complement id="off-map">
        <everywhere/>
        <region id="obs-spawn"/>
    </complement>
    <apply use="deny-trapdoor-use" region="spawns"/>
    <apply use="deny-beacon-use"/>
    <!-- apply block-break="only-mineable" block-place="deny-players-tnt" message="You may not destroy the spawn area!" region="spawns"/ -->
    <apply block="deny-players-tnt" message="You may not destroy the spawn region!" region="spawns"/>
    <apply block="never" message="You may not destroy the enchantment hut!" region="enchantment-huts"/>
    <apply block="never" message="You may not build outside of the map!" region="obs-spawn"/>
    <apply block-place="never" message="You may not build outside of the map!" region="off-map"/>
    <apply block-break="no-break" message="You may not break this block!"/>
</regions>
<destroyables materials="obsidian" mode-changes="true">
    <destroyables name="Hill Monument">
        <destroyable owner="green">
            <region>
                <cuboid min="20,43,146" max="23,46,149"/>
            </region>
        </destroyable>
        <destroyable owner="orange">
            <region>
                <cuboid min="-19,43,-183" max="-22,46,-186"/>
            </region>
        </destroyable>
    </destroyables>
    <destroyables name="River Monument">
        <destroyable owner="green">
            <region>
                <cuboid min="-11,24,106" max="-14,27,103"/>
            </region>
        </destroyable>
        <destroyable owner="orange">
            <region>
                <cuboid min="12,24,-142" max="15,27,-139"/>
            </region>
        </destroyable>
    </destroyables>
</destroyables>
<modes>
    <mode after="30m" material="beacon"/>
    <mode after="60m" material="coal block"/>
</modes>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>diamond pickaxe</tool>
    <tool>iron axe</tool>
    <tool>iron spade</tool>
    <tool>arrow</tool>
</toolrepair>
<itemremove>
    <item>wood</item>
    <item>stone:5</item>
    <item>golden carrot</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>obsidian</item>
    <item>beacon</item>
    <item>coal block</item>
    <item>flint</item>
</itemremove>
<itemkeep>
    <item>golden apple</item>
</itemkeep>
<kill-reward>
    <item material="golden apple"/>
    <kit id="kill-reward"/>
</kill-reward>
<block-drops>
    <rule>
        <filter>
            <material>gravel</material>
        </filter>
        <drops>
            <item>gravel</item>
        </drops>
    </rule>
    <rule>
        <filter>
            <material>lapis ore</material>
        </filter>
        <drops>
            <item amount="2" damage="4" material="ink sack"/>
        </drops>
        <experience>3</experience>
    </rule>
</block-drops>
<crafting>
    <!-- bulkcrafting; credits to Ruediger_LP. sourced from Columbia CTW XML. -->
    <shaped override="false">
        <result material="gold helmet" amount="9"/>
        <shape>
            <row>III</row>
            <row>I.I</row>
        </shape>
        <ingredient symbol="I">gold block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="gold chestplate" amount="9"/>
        <shape>
            <row>I.I</row>
            <row>III</row>
            <row>III</row>
        </shape>
        <ingredient symbol="I">gold block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="gold leggings" amount="9"/>
        <shape>
            <row>III</row>
            <row>I.I</row>
            <row>I.I</row>
        </shape>
        <ingredient symbol="I">gold block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="gold boots" amount="9"/>
        <shape>
            <row>I.I</row>
            <row>I.I</row>
        </shape>
        <ingredient symbol="I">gold block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="iron helmet" amount="9"/>
        <shape>
            <row>III</row>
            <row>I.I</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="iron chestplate" amount="9"/>
        <shape>
            <row>I.I</row>
            <row>III</row>
            <row>III</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="iron leggings" amount="9"/>
        <shape>
            <row>III</row>
            <row>I.I</row>
            <row>I.I</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="iron boots" amount="9"/>
        <shape>
            <row>I.I</row>
            <row>I.I</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="bucket" amount="9"/>
        <shape>
            <row>I.I</row>
            <row>.I.</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
</crafting>
<maxbuildheight>50</maxbuildheight>
<world>
    <timeset>1</timeset>
</world>
<gamerules>
    <doFireTick>false</doFireTick>
</gamerules>
</map>
