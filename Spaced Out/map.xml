<?xml version="1.0"?>
<map proto="1.4.0">
<name>Spaced out</name>
<version>1.0.0</version>
<gamemode>koth</gamemode>
<objective>Gain 750 points to win!</objective>
<authors>
    <author uuid="5198dc37-537f-4ea5-8ee8-1e0952d4a36b"/> <!--  _3Each  -->
    <author uuid="9116d3fa-1400-4ada-aecd-2eae1d73e383"/> <!--  Xuph  -->
</authors>
<contributors>
    <contributor uuid="e2d2c2c6-cea9-4510-9ab2-a091b5605b30" contribution="xml"/> <!--  Ruediger_LP  -->
</contributors>
<teams>
    <team id="blue-team" color="blue" max="5">Blue</team>
    <team id="red-team" color="red" max="5">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" material="stone sword" unbreakable="true"/>
        <item slot="1" material="bow" unbreakable="true"/>
        <item slot="2" material="arrow" amount="16" />
        <item slot="8" material="golden apple"/>
        <chestplate material="chainmail chestplate" locked="true" unbreakable="true"/>
        <boots material="iron boots" locked="true" unbreakable="true">
            <enchantment level="2">protection projectile</enchantment>
        </boots>
        <game-mode>adventure</game-mode>
        <effect duration="oo" amplifier="100">damage resistance</effect>
    </kit>
    <kit force="true" id="reset-resitance-kit">
        <effect duration="0" amplifier="0">damage resistance</effect>
    </kit>    
    <kit id="blue-kit" parents="spawn-kit">
        <helmet material="leather helmet" color="0066cc" locked="true" unbreakable="true"/>
        <leggings material="leather leggings" color="0066cc" locked="true" unbreakable="true"/>
    </kit>
    <kit id="red-kit" parents="spawn-kit">
        <helmet material="leather helmet" color="cd0000" locked="true" unbreakable="true"/>
        <leggings material="leather leggings" color="cd0000" locked="true" unbreakable="true"/>
    </kit>
</kits>
<spawns>
    <spawn team="red-team" kit="red-kit" yaw="-90">
        <region>
            <point>-63.5,34,-27.5</point>
        </region>
    </spawn>
    <spawn team="blue-team" kit="blue-kit" yaw="90">
        <region>
            <point>34.5,34,-27.5</point>
        </region>
    </spawn>
    <default yaw="90">
        <region>
            <cylinder base="20.5,29,-27.5" radius="2" height="0"/>
        </region>
    </default>
</spawns>
<respawn delay="3s"/>
<control-points visual-materials="visual-materials-filter" neutral-state="true" incremental="true" capture-time="5s" points="1">
    <control-point name="South" captured="south-captured" capture="south-capture" progress="south-progress" id="south-point"/>
    <control-point name="Middle" captured="middle-captured" capture="middle" progress="middle"/>
    <control-point name="North" captured="north-captured" capture="north-capture" progress="north-progress" id="north-point"/>
</control-points>
<score>
    <kills>0</kills>
    <death>0</death>
    <limit>750</limit>
</score>
<filters>
    <team id="only-blue">blue-team</team>
    <team id="only-red">red-team</team>
    <objective id="south-point-filter">south-point</objective>
    <objective id="north-point-filter">north-point</objective>
    <any id="visual-materials-filter">
        <material>wool:0</material>
        <material>wool:11</material>
        <material>wool:14</material>
        <material>stained glass:0</material>
        <material>stained glass:11</material>
        <material>stained glass:14</material>
    </any>
</filters>
<regions>
    <cuboid id="middle" min="-18,21,-31" max="-11,24,-24"/>
    <cuboid id="middle-captured" min="-18,36,-34" max="-11,54,-21"/>
    <cylinder id="south-progress" base="-14.5,14,0.5" radius="7.5" height="1"/>
    <cylinder id="south-capture" base="-14.5,14,0.5" radius="7.5" height="3"/>
    <cylinder id="south-captured" base="-14.5,14,0.5" radius="7.5" height="23"/>
    <cylinder id="north-progress" base="-14.5,14,-55.5" radius="7.5" height="1"/>
    <cylinder id="north-capture" base="-14.5,14,-55.5" radius="7.5" height="3"/>
    <cylinder id="north-captured" base="-14.5,14,-55.5" radius="7.5" height="23"/>
    <negative id="not-spawns">
        <union>
            <cylinder base="-63.5,19,-27.5" radius="1.5" height="5"/>
            <cylinder base="34.5,19,-27.5" radius="1.5" height="5"/>
        </union>
    </negative>
    <cuboid id="south-jumppad-1" min="-16,17,-1" max="-13,18,1"/>
    <cuboid id="south-jumppad-2" min="-16,27,-1" max="-13,30,2"/>
    <cuboid id="north-jumppad-1" min="-16,17,-56" max="-13,18,-54"/>
    <cuboid id="north-jumppad-2" min="-16,27,-57" max="-13,30,-54"/>
    <!-- applicators -->
    <apply velocity="0,3,0" region="south-jumppad-1" filter="south-point-filter"/>
    <apply velocity="0,3,0" region="north-jumppad-1" filter="north-point-filter"/>
    <apply velocity="0,0,-1" region="south-jumppad-2" filter="south-point-filter"/>
    <apply velocity="0,0,1" region="north-jumppad-2" filter="north-point-filter"/>
    <apply block="never" region="everywhere" message="You may not edit the Map!"/>
    <apply kit="reset-resitance-kit" region="not-spawns"/>
</regions>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemkeep>
    <item>stone sword</item>
    <item>bow</item>
    <item>arrow</item>
    <item>golden apple</item>
</itemkeep>
<itemremove>
    <item>leather helmet</item>
    <item>chainmail chestplate</item>
    <item>leather leggings</item>
    <item>iron boots</item>
</itemremove>
<hunger>
    <depletion>off</depletion>
</hunger>
<kill-rewards>
    <kill-reward>
        <item material="golden apple"/>
        <item material="arrow" amount="8"/>
    </kill-reward>
</kill-rewards>
</map>
