<map proto="1.4.2">
<name>Huanxiang</name>
<version>1.0</version>
<objective>Capture the enemy team's two wools!</objective>
<gamemode>ctw</gamemode>
<authors>
    <author uuid="f1ff7ecc-2e79-4320-85de-adb392eb2822"/> <!-- DerStiv -->
</authors>
<contributors>
    <contributor uuid="6863869b-4b8c-4445-b778-a8e016775ae4" contribution="XML"/> <!-- Cazadorian -->
</contributors>
<teams>
    <team id="blue" color="blue" max="12">Blue</team>
    <team id="red" color="dark red" max="12">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" unbreakable="true" material="iron sword"/>
        <item slot="1" unbreakable="true" material="bow"/>
        <item slot="2" unbreakable="true" material="iron pickaxe"/>
        <item slot="28" amount="64" material="arrow"/>
        <item slot="3" unbreakable="true" material="iron axe"/>
        <item slot="4" amount="64" material="log" damage="3"/>
        <item slot="7" material="bucket"/>
        <item slot="8" amount="64" material="golden carrot"/>
        <effect duration="4" amplifier="255">damage resistance</effect>
        <helmet unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
        <leggings material="leather leggings" team-color="true" unbreakable="true"/>
        <boots material="leather boots" team-color="true" unbreakable="true"/>
    </kit>
    <kit id="blue-kit" parent="spawn-kit">
        <item slot="5" material="stained clay" damage="11" amount="64"/>
        <item slot="27" material="stained clay" damage="11" amount="64"/>
    </kit>
    <kit id="red-kit" parent="spawn-kit">
        <item slot="5" material="stained clay" damage="14" amount="64"/>
        <item slot="27" material="stained clay" damage="14" amount="64"/>
    </kit>
</kits>
<spawns>
    <default yaw="0">
        <regions>
            <point>148.5,20,-49.5</point>
        </regions>
    </default>
    <spawn team="red" kit="red-kit" yaw="-90">
        <regions>
            <point>75.5,9,7.5</point>
        </regions>
    </spawn>
    <spawn team="blue" kit="blue-kit" yaw="90">
        <regions>
            <point>221.5,9,7.5</point>
        </regions>
    </spawn>
</spawns>
<filters>
    <deny id="deny-players">
          <participating/>
    </deny>
    <material id="only-iron">iron block</material>
    <not id="blue-in-wr">
        <any>
            <team id="only-red">red</team>
            <material>chest</material>
            <material>stained clay:14</material>
            <material>stained clay:9</material>
            <material>jungle wood stairs</material>
            <material>step</material>
            <material>trap door</material>
            <material>note block</material>
            <material>jungle fence</material>
            <material>stained glass</material>
            <material>prismarine</material>
            <material>wool</material>
            <material>leaves</material>
            <material>stained glass pane</material>
            <material>daylight detector</material>
            <material>daylight detector inverted</material>
            <material>sea lantern</material>
            <material>smooth stairs</material>
            <material>double step</material>
            <material>smooth brick</material>
        </any>
    </not>
    <not id="red-in-wr">
        <any>
            <team id="only-blue">blue</team>
            <material>chest</material>
            <material>stained clay:11</material>
            <material>stained clay:9</material>
            <material>jungle wood stairs</material>
            <material>step</material>
            <material>trap door</material>
            <material>note block</material>
            <material>jungle fence</material>
            <material>stained glass</material>
            <material>prismarine</material>
            <material>wool</material>
            <material>leaves</material>
            <material>stained glass pane</material>
            <material>daylight detector</material>
            <material>daylight detector inverted</material>
            <material>sea lantern</material>
            <material>smooth stairs</material>
            <material>double step</material>
            <material>smooth brick</material>
        </any>
    </not>
<not id="only-blue-usage">
    <any>
        <filter id="only-red"/>
        <material>beacon</material>
    </any>
</not>
<not id="only-red-usage">
    <any>
        <filter id="only-blue"/>
        <material>beacon</material>
    </any>
</not>
    <not id="deny-void">
        <void/>
    </not>
</filters>
<regions>
    <union id="spawn-protection">
        <rectangle id="red-protection" min="84,1" max="70,14"/>
        <rectangle id="blue-protection" min="213,1" max="227,14"/>
    </union>
    <complement id="void-area">
        <everywhere/>
        <!-- Middle-->
        <rectangle min="107,42" max="190,-27"/>
        <region id="spawn-protection"/>
        <region id="wool-rooms"/>
    </complement>
    <union id="wool-rooms">
        <union id="blue-wool-rooms"> <!-- blue defends -->
            <rectangle min="216,-8" max="226,1"/> <!-- Purple -->
            <rectangle min="216,23" max="226,14"/> <!-- Pink -->
        </union>
        <union id="red-wool-rooms"> <!-- red defends -->
            <rectangle min="80,-8" max="71,1"/> <!-- Yellow -->
            <rectangle min="80,23" max="71,14"/> <!-- Orange -->
        </union>
    </union>
    <apply enter="only-blue" region="blue-protection" message="You may not enter the opponent's spawn!"/>
    <apply enter="only-red" region="red-protection" message="You may not enter the opponent's spawn!"/>
    <apply enter="only-blue" region="red-wool-rooms" message="You may not enter your own wool room!"/>
    <apply enter="only-red" region="blue-wool-rooms" message="You may not enter your own wool room!"/>
    <apply block="blue-in-wr" use="only-blue-usage" region="red-wool-rooms" message="You may not modify the wool rooms!"/>
    <apply block="red-in-wr" use="only-red-usage" region="blue-wool-rooms" message="You may not modify the wool rooms!"/>
    <apply block-place="deny-void" region="void-area" message="You may not build in the void area!"/>
    <apply block-break="never" block-place="never" region="spawn-protection" message="You may not modify the spawns!"/>
    <apply block-break="never" block-place="never" region="gapspawner-protection" message="You may not break the spawner!"/>
    <cuboid id="gapple" min="147.5,6,8.5" max="149.5,9,6.5"/>
    <cuboid id="gapple-point" min="148.5,8.5,7.5" max="148.5,8.5,7.5"/>
    <cuboid id="gapspawner-protection" min="148.5,7,7.5" max="148.5,10,7.5"/>
</regions>
<spawners>
    <spawner spawn-region="gapple-point" player-region="gapple" delay="5s">
        <item amount="1" material="golden apple"/>
    </spawner>
</spawners>
<wools team="red" craftable="false">
    <wool color="pink" location="221,7,21">
        <monument>
            <block>75,10,13</block>
        </monument>
    </wool>
    <wool color="purple" location="221,7,-6">
        <monument>
            <block>75,10,1</block>
        </monument>
    </wool>
</wools>
<wools team="blue" craftable="false">
    <wool color="orange" location="78,10,58">
        <monument>
            <block>221,10,13</block>
        </monument>
   </wool>
    <wool color="yellow" location="78,10,-53">
        <monument>
            <block>221,10,1</block>
        </monument>
    </wool>
</wools>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>iron pickaxe</tool>
    <tool>iron axe</tool>
</toolrepair>
<itemremove>
    <item>golden carrot</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <!-- long grass, all types -->
    <item>31</item>
    <!-- "red rose", all types -->
    <item>38</item>
    <item>175</item>
    <item>sugar cane</item>
    <item>emerald</item>
    <item>yellow flower</item>
    <item>string</item>
    <item>redstone</item>
    <item>redstone torch on</item>
    <item>seeds</item>
    <item>gold plate</item>
    <item>torch</item>
</itemremove>
<block-drops>
    <rule>
        <filter>
            <material>wood</material>
        </filter>
        <drops>
            <item chance="0" material="wood"/>
        </drops>
    </rule>
    <rule>
        <filter>
            <material>stained clay</material>
        </filter>
        <drops>
            <item chance="0" material="stained clay"/>
        </drops>
    </rule>
</block-drops>
<itemkeep>
    <item>log</item>
    <item>stained clay</item>
    <item>golden apple</item>
</itemkeep>
<kill-reward>
    <item amount="32" material="wood"/>
    <item amount="16" material="glass"/>
    <item material="golden apple"/>
</kill-reward>
<world>
    <timeset>6000</timeset>
</world>
<maxbuildheight>23</maxbuildheight>
</map> 
