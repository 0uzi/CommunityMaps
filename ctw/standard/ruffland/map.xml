<?xml version="1.0"?>
<map proto="1.4.0">
<name>Ruffland</name>
<version>1.0.0</version>
<objective>Capture The Wool</objective>
<gamemode>ctw</gamemode>
<created>2022-03-17</created>
<authors>
    <author uuid="2ee523ce-6bf2-430f-aba5-2a2cd8608f64"/>
    <author uuid="f6f5dce8-a158-461d-b26d-dc8acfaeedd7"/>
</authors>
<teams>
    <team id="blue-team" color="blue" max="12" max-overfill="16">Blue</team>
    <team id="green-team" color="green" max="12" max-overfill="16">Green</team>
    <team id="red-team" color="red" max="12" max-overfill="16">Red</team>
    <team id="yellow-team" color="yellow" max="12" max-overfill="16">Yellow</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" material="stone sword" unbreakable="true"/>
        <item slot="1" material="bow" unbreakable="true" enchantment="arrow infinite"/>
        <item slot="2" material="diamond pickaxe" unbreakable="true"/>
        <item slot="3" material="iron axe" unbreakable="true"/>
        <item slot="4" material="wood" amount="64"/>
        <item slot="5" material="glass" amount="64"/>
        <item slot="8" material="golden apple"/>
        <item slot="28" material="arrow"/>
        <item slot="30" material="iron spade" unbreakable="true"/>
        <leggings material="chainmail leggings" unbreakable="true"/>
        <effect duration="oo" amplifier="100">damage resistance</effect>
    </kit>
    <kit force="true" id="reset-resistance-kit">
        <effect duration="0" amplifier="0">damage resistance</effect>
    </kit>
    <kit id="blue-kit" parent="spawn-kit">
        <helmet material="leather helmet" color="3F3FFF" unbreakable="true"/>
        <chestplate material="leather chestplate" color="3F3FFF" unbreakable="true"/>
        <boots material="leather boots" color="3F3FFF" unbreakable="true"/>
    </kit>
    <kit id="green-kit" parent="spawn-kit">
        <helmet material="leather helmet" color="3FFF3F" unbreakable="true"/>
        <chestplate material="leather chestplate" color="3FFF3F" unbreakable="true"/>
        <boots material="leather boots" color="3FFF3F" unbreakable="true"/>
    </kit>
    <kit id="red-kit" parent="spawn-kit">
        <helmet material="leather helmet" color="FF3F3F" unbreakable="true"/>
        <chestplate material="leather chestplate" color="FF3F3F" unbreakable="true"/>
        <boots material="leather boots" color="FF3F3F" unbreakable="true"/>
    </kit>
    <kit id="yellow-kit" parent="spawn-kit">
        <helmet material="leather helmet" color="FFFF0C" unbreakable="true"/>
        <chestplate material="leather chestplate" color="FFFF0C" unbreakable="true"/>
        <boots material="leather boots" color="FFFF0C" unbreakable="true"/>
    </kit>
</kits>
<spawns>
    <spawn team="blue-team" kit="blue-kit" yaw="0" region="blue-spawn-point"/>
    <spawn team="green-team" kit="red-kit" yaw="90" region="green-spawn-point"/>
    <spawn team="red-team" kit="red-kit" yaw="180" region="red-spawn-point"/>
    <spawn team="yellow-team" kit="red-kit" yaw="-90" region="yellow-spawn-point"/>
    <default yaw="180" region="obs-spawn-point"/>
</spawns>
<wools craftable="false">
    <!-- blue -->
    <wool team="blue-team" color="lime" monument="blue-team-lime-wool" location="-26.5,19.2,-128.5"/>
    <wool team="blue-team" color="pink" monument="blue-team-pink-wool" location="-129.5,19.2,-26.5"/>
    <wool team="blue-team" color="orange" monument="blue-team-orange-wool" location="27.5,19.2,129.5"/>
    <!-- green -->
    <wool team="blue-team" color="light-blue" monument="green-team-light-blue-wool" location="-128.5,19.2,27.5"/>
    <wool team="blue-team" color="pink" monument="green-team-pink-wool" location="-129.5,19.2,-26.5"/>
    <wool team="blue-team" color="orange" monument="green-team-orange-wool" location="27.5,19.2,129.5"/>
    <!-- red -->
    <wool team="blue-team" color="lime" monument="red-team-lime-wool" location="-26.5,19.2,-128.5"/>
    <wool team="blue-team" color="light-blue" monument="red-team-light-blue-wool" location="-128.5,19.2,27.5"/>
    <wool team="blue-team" color="orange" monument="red-team-orange-wool" location="27.5,19.2,129.5"/>
    <!-- yellow -->
    <wool team="blue-team" color="lime" monument="yellow-team-lime-wool" location="-26.5,19.2,-128.5"/>
    <wool team="blue-team" color="pink" monument="yellow-team-pink-wool" location="-129.5,19.2,-26.5"/>
    <wool team="blue-team" color="light-blue" monument="yellow-team-light-blue-wool" location="-128.5,19.2,27.5"/>
</wools>
<filters>
    <not id="not-blue">
        <team id="only-blue">blue-team</team>
    </not>
    <not id="not-green">
        <team id="only-green">green-team</team>
    </not>
    <not id="not-red">
        <team id="only-red">red-team</team>
    </not>
    <not id="not-yellow">
        <team id="only-yellow">yellow-team</team>
    </not>
    <any id="block-break-void-filter">
        <all>
            <any>
                <material>leaves</material>
                <material>log</material>
                <material>wool</material>
                <material>stained glass</material>
                <material>spruce fence</material>
                <material>stained clay</material>
                <material>wood step</material>
            </any>
            <void/>
        </all>
        <not id="block-place-void-filter">
            <void/>
        </not>
    </any>
    <all id="light-blue-woolroom-filter">
        <not>
            <team>blue-team</team>
        </not>
        <filter id="woolroom-filter"/>
    </all>
    <all id="lime-woolroom-filter">
        <not>
            <team>green-team</team>
        </not>
        <filter id="woolroom-filter"/>
    </all>
    <all id="pink-woolroom-filter">
        <not>
            <team>red-team</team>
        </not>
        <filter id="woolroom-filter"/>
    </all>
    <all id="orange-woolroom-filter">
        <not>
            <team>yellow-team</team>
        </not>
        <filter id="woolroom-filter"/>
    </all>
    <any id="woolroom-filter">
        <material>web</material>
        <material>wood:3</material>
        <material>stained clay</material>
        <material>gold block</material>
        <material>water</material>
        <material>stationary water</material>
        <material>workbench</material>
    </any>
    <material id="only-wool-1">wool:1</material>
    <material id="only-wool-3">wool:3</material>
    <material id="only-wool-5">wool:5</material>
    <material id="only-wool-6">wool:6</material>
    <allow id="allow-wool">
        <material>wool</material>
    </allow>
    <all id="only-iron-cause-world">
        <material id="only-iron">iron block</material>
        <cause>world</cause>
    </all>
    <material id="only-air">air</material>
</filters>
<regions>
    <!--spawns-->
    <cylinder id="blue-spawn-point" base="-93.5,13,-29.5" radius="1" height="0"/>
    <cylinder id="green-spawn-point" base="30.5,13,-93.5" radius="1" height="0"/>
    <cylinder id="red-spawn-point" base="94.5,13,30.5" radius="1" height="0"/>
    <cylinder id="yellow-spawn-point" base="-29.5,13,94.5" radius="1" height="0"/>
    <cylinder id="obs-spawn-point" base="0.5,59,0.5" radius="2" height="0"/>
    <!--wool monuments-->
    <!--blue-->
    <block id="blue-team-lime-wool">-95,13,-35</block>
    <block id="blue-team-pink-wool">-94,13,-35</block>
    <block id="blue-team-orange-wool">-93,13,-35</block>
    <!--green-->
    <block id="green-team-pink-wool">35,13,-95</block>
    <block id="green-team-orange-wool">35,13,-94</block>
    <block id="green-team-light-blue-wool">35,13,-93</block>
    <!--red-->
    <block id="red-team-lime-wool">93,13,35</block>
    <block id="red-team-light-blue-wool">94,13,35</block>
    <block id="red-team-orange-wool">95,13,35</block>
    <!--yellow-->
    <block id="yellow-team-lime-wool">-35,13,94</block>
    <block id="yellow-team-pink-wool">-35,13,93</block>
    <block id="yellow-team-light-blue-wool">-35,13,95</block>
    <!-- wool spawns -->
    <point id="light-blue-wool-spawn">-128.5,19.2,27.5</point>
    <point id="lime-wool-spawn">-26.5,19.2,-128.5</point>
    <point id="pink-wool-spawn">-129.5,19.2,-26.5</point>
    <point id="orange-wool-spawn">27.5,19.2,129.5</point>
    <!-- -->
    <negative id="not-spawns">
        <union id="spawns">
            <rectangle id="blue-spawn" min="-119,7" max="-86,-40"/>
            <rectangle id="green-spawn" min="-6,-119" max="41,-86"/>
            <rectangle id="red-spawn" min="120,-6" max="87,41"/>
            <rectangle id="yellow-spawn" min="7,120" max="-40,87"/>
        </union>
    </negative>
    <union id="woolrooms">
        <rectangle id="light-blue-woolroom" min="-120,16" max="-137,39"/>
        <rectangle id="lime-woolroom" min="-15,-120" max="-38,-137"/>
        <rectangle id="pink-woolroom" min="121,-15" max="138,-38"/>
        <rectangle id="orange-woolroom" min="16,121" max="39,138"/>
    </union>
    <!--build area-->
    <negative id="not-build-area">
        <union id="bluild-area">
            <circle center="0.5,0.5" radius="70"/> <!--MID-->
            <rectangle min="-112,54" max="-48,27"/> <!--blue wool lane-->
            <rectangle min="-53,-112" max="-26,-48"/> <!--green wool lane-->
            <rectangle min="49,-26" max="113,-53"/> <!--red wool lane-->
            <rectangle min="27,49" max="53,113"/> <!--yellow wool lane-->
        </union>
    </negative>
    <union id="iron-regen">
        <cuboid min="-98,7" max="-118,-13"/> <!-- blue iron -->
        <cuboid min="-6,-98" max="14,-118"/> <!-- green iron -->
        <cuboid min="99,-6" max="119,14"/> <!-- red iron -->
        <cuboid min="7,99" max="-13,119"/> <!-- yellow iron -->
    </union>
    <!-- applicators -->
    <apply enter="only-blue" region="blue-spawn" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-green" region="green-spawn" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-red" region="red-spawn" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-yellow" region="yellow-spawn" message="You may not enter the enemy's spawn!"/>
    <apply enter="not-blue" region="light-blue-woolroom" message="You may not enter your own wool room!"/>
    <apply enter="not-green" region="lime-woolroom" message="You may not enter your own wool room!"/>
    <apply enter="not-red" region="pink-woolroom" message="You may not enter your own wool room!"/>
    <apply enter="not-yellow" region="orange-woolroom" message="You may not enter your own wool room!"/>
    <apply block="light-blue-woolroom-filter" region="light-blue-woolroom" message="You may not edit the wool room!"/>
    <apply block="lime-woolroom-filter" region="lime-woolroom" message="You may not edit the wool room!"/>
    <apply block="pink-woolroom-filter" region="pink-woolroom" message="You may not edit the wool room!"/>
    <apply block="orange-woolroom-filter" region="orange-woolroom" message="You may not edit the wool room!"/>
    <apply block-place="never" block-break="allow-block-break-spawn" region="spawns" message="You may not edit spawn!"/>
</regions>
<spawners>
    <spawner player-region="light-blue-woolroom" spawn-region="light-blue-wool-spawn" delay="3s">
        <item material="wool" amount="3" damage="3"/>
    </spawner>
    <spawner player-region="lime-woolroom" spawn-region="lime-wool-spawn" delay="3s">
        <item material="wool" amount="3" damage="10"/>
    </spawner>
    <spawner player-region="pink-woolroom" spawn-region="pink-wool-spawn" delay="3s">
        <item material="wool" amount="3" damage="14"/>
    </spawner>
    <spawner player-region="orange-woolroom" spawn-region="orange-wool-spawn" delay="3s">
        <item material="wool" amount="3" damage="5"/>
    </spawner>
</spawners>
<block-drops>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-1">
        <drops>
            <item damage="3" material="wool"/>
        </drops>
        <replacement>wool:3</replacement>
    </rule>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-3">
        <drops>
            <item damage="10" material="wool"/>
        </drops>
        <replacement>wool:10</replacement>
    </rule>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-5">
        <drops>
            <item damage="14" material="wool"/>
        </drops>
        <replacement>wool:14</replacement>
    </rule>
    <rule wrongtool="false" region="woolrooms" filter="only-wool-6">
        <drops>
            <item damage="5" material="wool"/>
        </drops>
        <replacement>wool:5</replacement>
    </rule>
</block-drops>
<itemkeep>
    <item>stone sword</item>
    <item>bow</item>
    <item>diamond pickaxe</item>
    <item>iron axe</item>
    <item>arrow</item>
    <item>iron spade</item>
    <item>wood</item>
    <item>glass</item>
    <item>golden apple</item>
</itemkeep>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
    <item>stone</item>
    <item>string</item>
    <item>red rose</item>
    <item>long grass</item>
    <item>sand</item>
    <item>wood step</item>
</itemremove>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
    <tool>diamond pickaxe</tool>
    <tool>iron axe</tool>
    <tool>arrow</tool>
    <tool>iron spade</tool>
</toolrepair>
<hunger>
    <depletion>off</depletion>
</hunger>
<maxbuildheight>32</maxbuildheight>
<kill-rewards>
    <kill-reward>
        <item material="golden apple" amount="1"/>
        <item material="wood" amount="16"/>
    </kill-reward>
</kill-rewards>
</map>
