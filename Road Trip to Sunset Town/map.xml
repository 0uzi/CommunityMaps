<map proto="1.4.0">
<name>Road Trip to Sunset Town</name>
<version>1.0.2</version>
<objective>Capture the other three teams' wools.</objective>
<authors>
    <author uuid="f876b863-1ccd-4ae2-acad-8a09e2209e81"/>
    <!--  _DISTRACTION_  -->
    <author uuid="63b88dff-c6b4-4e08-bd17-aae089b12e95"/>
    <!--  Riilo  -->
    <author uuid="0b8f95fa-0f40-4f28-9fd0-b8e243bb6800"/>
    <!--  Fouled  -->
    <author uuid="89f514e1-f0f5-4977-8ddc-77ef030a8b46"/>
    <!--  Posighdun  -->
</authors>
<teams>
    <team id="red" color="dark red" max="16">Red</team>
    <team id="yellow" color="yellow" max="16">Yellow</team>
    <team id="blue" color="blue" max="16">Blue</team>
    <team id="lime" color="green" max="16">Lime</team>
</teams>
<spawns>
    <spawn team="red" kit="red-kit">
        <region yaw="90">
            <cylinder base="117.5,13,0.5" height="0" radius="0"/>
        </region>
    </spawn>
    <spawn team="yellow" kit="yellow-kit">
        <region yaw="180">
            <cylinder base="0.5,13,117.5" height="0" radius="0"/>
        </region>
    </spawn>
    <spawn team="blue" kit="blue-kit">
        <region yaw="-90">
            <cylinder base="-116.5,13,0.5" height="0" radius="0"/>
        </region>
    </spawn>
    <spawn team="lime" kit="lime-kit">
        <region yaw="0">
            <cylinder base="0.5,13,-116.5" height="0" radius="0"/>
        </region>
    </spawn>
    <default yaw="0">
        <region>
            <cuboid min="0.5,75,-0.5" max="0.5,75,-0.5"/>
        </region>
    </default>
</spawns>
<kits>
    <kit id="spawn">
        <item slot="0" unbreakable="true">iron sword</item>
        <item slot="1" unbreakable="true">bow</item>
        <item slot="2" unbreakable="true" enchantment="dig speed:1">diamond pickaxe</item>
        <item slot="3" unbreakable="true" enchantment="dig speed:2">stone axe</item>
        <item slot="4" amount="24">log</item>
        <item slot="6" amount="2">golden apple</item>
        <item slot="7" amount="64">cooked fish</item>
        <item slot="8" unbreakable="true">stone spade</item>
        <item slot="26">flint and steel</item>
        <item slot="28" amount="48">arrow</item>
        <potion duration="5" amplifier="5">regeneration</potion>
    </kit>
    <kit id="red-kit" parents="spawn">
        <item slot="5" amount="32" damage="14">stained glass</item>
        <helmet unbreakable="true" color="993333" material="leather helmet"><enchantment level="1">protection explosions</enchantment></helmet>
        <chestplate unbreakable="true" color="993333" material="leather chestplate"><enchantment level="1">protection explosions</enchantment></chestplate>
        <leggings unbreakable="true" color="993333" material="leather leggings"><enchantment level="1">protection explosions</enchantment></leggings>
        <boots unbreakable="true" color="993333" material="leather boots"><enchantment level="1">protection explosions</enchantment></boots>
    </kit>
    <kit id="yellow-kit" parents="spawn">
        <item slot="5" amount="32" damage="4">stained glass</item>
        <helmet unbreakable="true" color="e5e533" material="leather helmet"><enchantment level="1">protection explosions</enchantment></helmet>
        <chestplate unbreakable="true" color="e5e533" material="leather chestplate"><enchantment level="1">protection explosions</enchantment></chestplate>
        <leggings unbreakable="true" color="e5e533" material="leather leggings"><enchantment level="1">protection explosions</enchantment></leggings>
        <boots unbreakable="true" color="e5e533" material="leather boots"><enchantment level="1">protection explosions</enchantment></boots>
    </kit>
    <kit id="blue-kit" parents="spawn">
        <item slot="5" amount="32" damage="11">stained glass</item>
        <helmet unbreakable="true" color="334cb2" material="leather helmet"><enchantment level="1">protection explosions</enchantment></helmet>
        <chestplate unbreakable="true" color="334cb2" material="leather chestplate"><enchantment level="1">protection explosions</enchantment></chestplate>
        <leggings unbreakable="true" color="334cb2" material="leather leggings"><enchantment level="1">protection explosions</enchantment></leggings>
        <boots unbreakable="true" color="334cb2" material="leather boots"><enchantment level="1">protection explosions</enchantment></boots>
    </kit>
    <kit id="lime-kit" parents="spawn">
        <item slot="5" amount="32" damage="5">stained glass</item>
        <helmet unbreakable="true" color="7fcc19" material="leather helmet"><enchantment level="1">protection explosions</enchantment></helmet>
        <chestplate unbreakable="true" color="7fcc19" material="leather chestplate"><enchantment level="1">protection explosions</enchantment></chestplate>
        <leggings unbreakable="true" color="7fcc19" material="leather leggings"><enchantment level="1">protection explosions</enchantment></leggings>
        <boots unbreakable="true" color="7fcc19" material="leather boots"><enchantment level="1">protection explosions</enchantment></boots>
    </kit>
</kits>
<wools craftable="false" location="0,0,0">
    <wools team="red">
        <wool color="yellow">
            <monument><block>119,14,0</block></monument>
        </wool>
        <wool color="blue">
            <monument><block>119,14,-1</block></monument>
        </wool>
        <wool color="lime">
            <monument><block>119,14,1</block></monument>
        </wool>
    </wools>
    <wools team="yellow">
        <wool color="red">
            <monument><block>0,14,119</block></monument>
        </wool>
        <wool color="blue">
            <monument><block>1,14,119</block></monument>
        </wool>
        <wool color="lime">
            <monument><block>-1,14,119</block></monument>
        </wool>
    </wools>
    <wools team="blue">
        <wool color="red">
            <monument><block>-119,14,0</block></monument>
        </wool>
        <wool color="yellow">
            <monument><block>-119,14,1</block></monument>
        </wool>
        <wool color="lime">
            <monument><block>-119,14,-1</block></monument>
        </wool>
    </wools>
    <wools team="lime">
        <wool color="red">
            <monument><block>1,14,-119</block></monument>
        </wool>
        <wool color="yellow">
            <monument><block>0,14,-119</block></monument>
        </wool>
        <wool color="blue">
            <monument><block>-1,14,-119</block></monument>
        </wool>
    </wools>
</wools>
<kill-reward>
    <item>golden apple</item>
    <item amount="8">arrow</item>
</kill-reward>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>diamond pickaxe</tool>
    <tool>stone axe</tool>
    <tool>stone spade</tool>
</toolrepair>
<itemremove>
    <item>cooked fish</item>
    <item>golden apple</item>
    <item>log</item>
    <item>wood</item>
    <item>stained glass</item>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>redstone</item>
    <item>map</item>
    <item>item frame</item>
    <item>snow ball</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>flint and steel</item>
</itemremove>
<filters>
    <not id="no-void">
        <any>
            <material>wool</material>
            <void/>
        </any>
    </not>
    <not id="not-red">
        <team id="only-red">red</team>
    </not>
    <not id="not-yellow">
        <team id="only-yellow">yellow</team>
    </not>
    <not id="not-blue">
        <team id="only-blue">blue</team>
    </not>
    <not id="not-lime">
        <team id="only-lime">lime</team>
    </not>
    <not id="valid-uses">
        <material>item frame</material>
    </not>
</filters>
<tnt>
    <instantignite>on</instantignite>
    <fuse>3s</fuse>
    <licensing>false</licensing>
    <blockdamage>true</blockdamage>
</tnt>
<regions>
    <rectangle id="all-wool-rooms" min="7,-6" max="-6,7"/>
    <intersect id="red-wool-room">
        <below x="7"/>
        <half origin="7.5,0,7.5" normal="1,0,-1"/>
        <half origin="7.5,0,-6.5" normal="1,0,1"/>
    </intersect>
    <intersect id="yellow-wool-room">
        <below z="7"/>
        <half origin="-6.5,0,7.5" normal="1,0,1"/>
        <half origin="7.5,0,7.5" normal="-1,0,1"/>
    </intersect>
    <intersect id="blue-wool-room">
        <above x="-6"/>
        <half origin="-6.5,0,-6.5" normal="-1,0,1"/>
        <half origin="-6.5,0,7.5" normal="-1,0,-1"/>
    </intersect>
    <intersect id="lime-wool-room">
        <above z="-6"/>
        <half origin="7.5,0,-6.5" normal="-1,0,-1"/>
        <half origin="-6.5,0,-6.5" normal="1,0,-1"/>
    </intersect>
    <union id="all-spawns">
        <rectangle id="red-spawn" min="99,-5" max="121,6"/>
        <rectangle id="yellow-spawn" min="6,99" max="-5,121"/>
        <rectangle id="blue-spawn" min="-98,6" max="-120,-5"/>
        <rectangle id="lime-spawn" min="-5,-98" max="6,-120"/>
    </union>
    <apply enter="not-red" message="You may not enter your own wool room!" region="red-wool-room"/>
    <apply enter="not-yellow" message="You may not enter your own wool room!" region="yellow-wool-room"/>
    <apply enter="not-blue" message="You may not enter your own wool room!" region="blue-wool-room"/>
    <apply enter="not-lime" message="You may not enter your own wool room!" region="lime-wool-room"/>
    <apply block="never" use="valid-uses" message="You may not modify this area." region="all-wool-rooms"/>
    <apply enter="only-red" message="You may not enter another team's spawn!" region="red-spawn"/>
    <apply enter="only-yellow" message="You may not enter another team's spawn!" region="yellow-spawn"/>
    <apply enter="only-blue" message="You may not enter another team's spawn!" region="blue-spawn"/>
    <apply enter="only-lime" message="You may not enter another team's spawn!" region="lime-spawn"/>
    <apply block="never" message="You may not modify the spawns." region="all-spawns"/>
    <apply block="no-void" message="You may not build or destroy blocks outside the map, or place safeties!" region="everywhere"/>
</regions>
<spawners delay="5s" player-region="all-wool-rooms">
    <spawner id="red-spawner">
        <spawn-region>
            <point>5.5,22.5,0.5</point>
        </spawn-region>
        <item amount="1" damage="14">wool</item>
    </spawner>
    <spawner id="yellow-spawner">
        <spawn-region>
            <point>0.5,22.5,5.5</point>
        </spawn-region>
        <item amount="1" damage="4">wool</item>
    </spawner>
    <spawner id="blue-spawner">
        <spawn-region>
            <point>-4.5,22.5,0.5</point>
        </spawn-region>
        <item amount="1" damage="11">wool</item>
    </spawner>
    <spawner id="lime-spawner">
        <spawn-region>
            <point>0.5,22.5,-4.5</point>
        </spawn-region>
        <item amount="1" damage="5">wool</item>
    </spawner>
</spawners>
<damage>
    <deny>
        <all>
            <attacker>
                <region id="all-wool-rooms"/>
            </attacker>
            <victim>
                <region id="all-wool-rooms"/>
            </victim>
        </all>
    </deny>
</damage>
<timelock>off</timelock>
<maxbuildheight>45</maxbuildheight>
</map>
